* {
  margin: 0;
  padding: 0;
  outline: none;
}
a:focus,
a:active {
  outline: none;
}
a,
a:focus,
a:hover {
  cursor: pointer;
  color: inherit;
  text-decoration: none;
}
div:focus,
div:hover {
  outline: none;
}
button {
  outline: none;
}
button:hover,
button:active {
  border-color: transparent;
}
